		"zod": "3.23.4"
	},
	"devDependencies": {
		"@lynxtaa/eslint-config": "0.9.0",
		"@lynxtaa/prettier-config": "0.2.0",
		"@next/bundle-analyzer": "14.2.3",
		"@swc/core": "1.5.0",
		"@tailwindcss/forms": "0.5.7",
		"@testing-library/dom": "10.0.0",
