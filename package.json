{
  "name": "scooter-travel-distance",
  "version": "0.1.0",
  "private": true,
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "repository": "https://github.com/lynxtaa/scooter-travel-distance.git",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=14.x"
  },
  "jest": {
    "resetMocks": true,
    "setupFiles": [
      "whatwg-fetch"
    ],
    "setupFilesAfterEnv": [
      "./jest/setupTests.ts"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@chakra-ui/react": "1.4.2",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "framer-motion": "4.1.2",
    "next": "10.1.3",
    "next-localization": "0.11.0",
    "next-seo": "^4.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.0.0",
    "react-spring": "9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/node-fetch": "2.5.9",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.23.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "msw": "0.28.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.2.3",
    "whatwg-fetch": "3.6.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "is-ci-cli test:coverage test:watch",
    "lint": "eslint --ext .tsx,.ts,.js components jest lib pages",
    "check-types": "tsc --noEmit",
    "check-format": "npm run prettier -- --check",
    "prettier": "prettier components jest lib locales pages *.{js,json,md}",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
}
